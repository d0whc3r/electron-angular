{
  "name": "procedimientosApp",
  "private": true,
  "devDependencies": {
    "autoprefixer-core": "^5.2",
    "cross-env": "*",
    "electron-packager": "*",
    "electron-prebuilt": "*",
    "electron-rebuild": "*",
    "electron-reload": "*",
    "grunt": "^1.0",
    "grunt-angular-templates": "^1.0",
    "grunt-concurrent": "^1.0",
    "grunt-contrib-clean": "^1.0",
    "grunt-contrib-compass": "^1.0.0",
    "grunt-contrib-concat": "^1.0",
    "grunt-contrib-connect": "^1.0",
    "grunt-contrib-copy": "^1.0",
    "grunt-contrib-cssmin": "^1.0",
    "grunt-contrib-htmlmin": "^1.0",
    "grunt-contrib-imagemin": "^1.0.0",
    "grunt-contrib-jshint": "^1.0",
    "grunt-contrib-uglify": "^1.0",
    "grunt-contrib-watch": "^1.0",
    "grunt-filerev": "^2.1",
    "grunt-google-cdn": "^0.4",
    "grunt-injector": "^1.0",
    "grunt-jscs": "^1.8",
    "grunt-newer": "^1.1",
    "grunt-ng-annotate": "^1.0",
    "grunt-postcss": "^0.8",
    "grunt-run": "*",
    "grunt-svgmin": "^2.0",
    "grunt-usemin": "^3.0",
    "grunt-wiredep": "^2.0",
    "jasmine-core": "^2.4",
    "jit-grunt": "^0.10",
    "jshint-stylish": "^1.0",
    "karma": "^1.1",
    "karma-jasmine": "^1.0",
    "karma-phantomjs-launcher": "^1.0",
    "phantomjs-prebuilt": "^2.1",
    "time-grunt": "^1.0"
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "electronOut": "dist-electron",
  "electronVersion": "1.2.6",
  "scripts": {
    "start": "./node_modules/.bin/grunt serve --force",
    "start-dist": "npm run grunt-build && ./node_modules/.bin/electron dist",
    "grunt-build": "./node_modules/.bin/grunt build --force",
    "electron-build": "./node_modules/.bin/electron-packager ./dist $npm_package_name --out=$npm_package_electronOut --prune --asar --platform=linux --arch=x64 --version=$npm_package_electronVersion --overwrite",
    "electron-build-win": "./node_modules/.bin/electron-packager ./dist $npm_package_name --out=$npm_package_electronOut --prune --asar --platform=win32 --arch=x64 --version=$npm_package_electronVersion --overwrite",
    "electron-app-build": "./node_modules/.bin/electron-packager ./app $npm_package_name --out=$npm_package_electronOut --prune --asar --platform=linux --arch=x64 --version=$npm_package_electronVersion --overwrite",
    "build": "npm run grunt-build && npm run electron-build",
    "build-win": "npm run grunt-build && npm run electron-build-win",
    "electron-build-all": "./node_modules/.bin/electron-packager ./dist $npm_package_name --out=$npm_package_electronOut --prune --asar --platform=linux,win32 --arch=ia32,x64 --version=$npm_package_electronVersion --overwrite",
    "build-all": "npm run grunt-build && npm run electron-build-all",
    "test": "./node_modules/.bin/karma start test/karma.conf.js",
    "rebuild-app": "./node_modules/.bin/electron-rebuild --version $npm_package_electronVersion --module-dir app/node_modules/ -f -p",
    "rebuild-node": "./node_modules/.bin/electron-rebuild --version $npm_package_electronVersion --module-dir ./node_modules/ -f -p",
    "rebuild": "npm run rebuild-node && npm run rebuild-app"
  }
}
